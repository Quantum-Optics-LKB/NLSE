	volume = {366},
	issn = {0036-8075, 1095-9203},
	url = {http://arxiv.org/abs/1807.07564},
	doi = {10.1126/science.aau6103},
	abstract = {Scale-invariant fluxes are the defining property of turbulent cascades, but their direct measurement is a notorious problem. Here we perform such a measurement for a direct energy cascade in a turbulent quantum gas. Using a time-periodic force, we inject energy at a large lengthscale and generate a cascade in a uniformly-trapped Bose gas. The adjustable trap depth provides a high-momentum cutoff \$k\_\{{\textbackslash}textrm\{D\}\}\$, which realises a synthetic dissipation scale. This gives us direct access to the particle flux across a momentum shell of radius \$k\_\{{\textbackslash}textrm\{D\}\}\$, and the tunability of \$k\_\{{\textbackslash}textrm\{D\}\}\$ allows for a clear demonstration of the zeroth law of turbulence: we observe that for fixed forcing the particle flux vanishes as \$k\_\{{\textbackslash}textrm\{D\}\}{\textasciicircum}\{-2\}\$ in the dissipationless limit \$k\_\{{\textbackslash}textrm\{D\}\}{\textbackslash}rightarrow {\textbackslash}infty\$, while the energy flux is independent of \$k\_\{{\textbackslash}textrm\{D\}\}\$. Moreover, our time-resolved measurements give unique access to the pre-steady-state dynamics, when the cascade front propagates in momentum space.},
	language = {en},
	number = {6463},
	urldate = {2022-04-27},
	journal = {Science},
	author = {Navon, Nir and Eigen, Christoph and Zhang, Jinyi and Lopes, Raphael and Gaunt, Alexander L. and Fujimoto, Kazuya and Tsubota, Makoto and Smith, Robert P. and Hadzibabic, Zoran},
	!month = oct,
	year = {2019},
	!note = {arXiv: 1807.07564},
	keywords = {Condensed Matter - Quantum Gases, Quantum Physics, Condensed Matter - Statistical Mechanics, Physics - Atomic Physics, Physics - Fluid Dynamics},
