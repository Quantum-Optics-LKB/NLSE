@article{huang_lead_2017,
  title      = {Lead Halide Perovskite Nanocrystals in the Research Spotlight: Stability and Defect Tolerance},
  volume     = {2},
  issn       = {2380-8195, 2380-8195},
  url        = {https://pubs.acs.org/doi/10.1021/acsenergylett.7b00547},
  doi        = {10.1021/acsenergylett.7b00547},
  shorttitle = {Lead Halide Perovskite Nanocrystals in the Research Spotlight},
  abstract   = {This Perspective outlines basic structural and optical properties of lead halide perovskite colloidal nanocrystals, highlighting diﬀerences and similarities between them and conventional {II}−{VI} and {III}−V semiconductor quantum dots. A detailed insight into two important issues inherent to lead halide perovskite nanocrystals then follows, namely, the advantages of defect tolerance and the necessity to improve their stability in environmental conditions. The defect tolerance of lead halide perovskites oﬀers an impetus to search for similar attributes in other related heavy metal-free compounds. We discuss the origins of the signiﬁcantly blue-shifted emission from {CsPbBr}3 nanocrystals and the synthetic strategies toward fabrication of stable perovskite nanocrystal materials with emission in the red and infrared parts of the optical spectrum, which are related to fabrication of mixed cation compounds guided by Goldschmidt tolerance factor considerations. We conclude with the view on perspectives of use of the colloidal perovskite nanocrystals for applications in backlighting of liquid-crystal {TV} displays.},
  pages      = {2071--2083},
  number     = {9},
  journal    = {{ACS} Energy Lett.},
  author     = {Huang, He and Bodnarchuk, Maryna I. and Kershaw, Stephen V. and Kovalenko, Maksym V. and Rogach, Andrey L.},
  urldate    = {2020-03-24},
  date       = {2017-09-08},
  langid     = {english},
  !note       = {tex.ids: huang2017Lead}
}

