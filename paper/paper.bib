  !note       = {tex.ids: {prainSuperradiantScatteringFluids}2019
                !publisher: American Physical Society},
  keywords   = {Physics - Optics, Condensed Matter - Quantum Gases, General Relativity and Quantum Cosmology},
  !file       = {Prain et al. - 2019 - Superradiant scattering in fluids of light.pdf:/home/tangui/snap/zotero-snap/common/Zotero/storage/JSZ7ID94/Prain et al. - 2019 - Superradiant scattering in fluids of light.pdf:application/pdf;Version soumise:/home/tangui/snap/zotero-snap/common/Zotero/storage/78J2QK86/Prain et al. - 2019 - Superradiant scattering in fluids of light.pdf:application/pdf;APS Snapshot:/home/tangui/snap/zotero-snap/common/Zotero/storage/87Z2R853/PhysRevD.100.html:text/html}
